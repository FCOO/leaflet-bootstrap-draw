{
  "name": "leaflet-bootstrap-draw",
  "version": "0.3.0",
  "homepage": "https://github.com/FCOO/leaflet-bootstrap-draw",
  "authors": [
    "Niels Holt"
  ],
  "description": "",
  "main": [
    "dist/leaflet-bootstrap-draw.js",
    "dist/leaflet-bootstrap-draw.css"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/FCOO/leaflet-bootstrap-draw.git"
  },
  "dependencies": {
    "jquery": "latest",
    "leaflet": "latest",
    "Leaflet.Geodesic": "henrythasler/Leaflet.Geodesic",
    "Leaflet.GeometryUtil": "makinacorpus/Leaflet.GeometryUtil#^0.8.1",
    "leaflet-bootstrap": "fcoo/leaflet-bootstrap#^1.0.0",
    "leaflet-latlng-geodesy": "fcoo/leaflet-latlng-geodesy#^1.0.0",
    "leaflet-marker-vessel": "fcoo/leaflet-marker-vessel#^1.0.0",
    "leaflet-latlng-point": "fcoo/leaflet-latlng-point#^1.0.0",
    "leaflet-polyline": "fcoo/leaflet-polyline#^1.0.0"
  },
  "overrides": {
    "Leaflet.Geodesic": {
      "dependencies": {
        "leaflet": "latest"
      }
    },
    "Leaflet.GeometryUtil": {
      "main": [
        "src/leaflet.geometryutil.js"
      ]
    },
    "_geodesy": {
      "main": [
        "latlon-spherical.js"
      ]
    }
  },
  "devDependencies": {
    "bourbon": "latest",
    "modernizr-mixin": "latest",
    "mathsass": "latest",
    "geodesy": "chrisveness/geodesy#^1.1.3"
  },
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "moduleType": [
    "es6"
  ]
}
